  <main>
  <form method="post" action="/sign_in">
    <div>
      <label for="username">Username:
        <input name="username" value="<%= params[:username] %>"/>
      </label>
    </div>
    <div>
      <label for="password">Password
        <input name="password" type="password" />
      </label>
    </div>
    <button type="submit">Sign In</button>
  </form>
</main>